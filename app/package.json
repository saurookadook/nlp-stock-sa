{
    "name": "nlpssa-frontend",
    "description": "Frontend for nlp-stock-sa application",
    "packageManager": "yarn@4.1.1",
    "version": "0.0.0",
    "engines": {
        "node": "^18.18.2",
        "yarn": "^4.1.0"
    },
    "scripts": {
        "build:dev": "tsc -b --verbose && webpack --mode=development",
        "build:prod": "webpack --mode=production --node-env=production",
        "build": "yarn build:prod",
        "watch": "webpack --mode=development --watch",
        "install:clean": "yarn cache clean && rm -rf .yarn/ dist/ node_modules/ && yarn install",
        "serve": "nodemon -w src --exec \"node --experimental-specifier-resolution=node --loader ./customTSNodeLoader.js src/server/index.ts\"",
        "start": "yarn build:dev && yarn serve"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "express-handlebars": "^7.1.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "web-vitals": "^2.1.4"
    },
    "devDependencies": {
        "@babel/core": "^7.24.3",
        "@babel/node": "^7.23.9",
        "@babel/plugin-transform-runtime": "^7.24.3",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.24.3",
        "@babel/preset-react": "^7.24.1",
        "@testing-library/dom": "^10.0.0",
        "@testing-library/jest-dom": "^5.16.3",
        "@testing-library/react": "^12.1.4",
        "@testing-library/user-event": "^13.5.0",
        "@types/babel__core": "^7",
        "@types/babel__preset-env": "^7",
        "@types/cors": "^2.8.17",
        "@types/eslint": "^8",
        "@types/express": "^4.17.21",
        "@types/jest": "^27.4.1",
        "@types/node": "^20.12.5",
        "@types/react": "^17.0.43",
        "@types/react-dom": "^17.0.14",
        "@typescript-eslint/eslint-plugin": "^7.0.1",
        "@typescript-eslint/parser": "^7.0.0",
        "@webpack-cli/generators": "^3.0.7",
        "babel-loader": "^9.1.3",
        "concurrently": "^8.2.2",
        "copy-webpack-plugin": "^12.0.2",
        "eslint": "^8.52.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-babel": "^5.3.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^28.2.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "html-webpack-plugin": "^5.6.0",
        "nodemon": "^3.0.3",
        "prettier": "^3.1.0",
        "prettier-eslint": "^16.1.2",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.3.3",
        "webpack": "^5.90.3",
        "webpack-assets-manifest": "^5.2.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.2",
        "webpack-node-externals": "^3.0.0",
        "workbox-webpack-plugin": "^7.0.0"
    }
}
