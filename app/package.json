{
    "name": "nlpssa-frontend",
    "description": "Frontend for nlp-stock-sa application",
    "packageManager": "yarn@4.1.1",
    "version": "0.0.0",
    "type": "module",
    "engines": {
        "node": "^18.20.2",
        "yarn": "^4.1.0"
    },
    "scripts": {
        "safe-rm-ycache": "rm -rf .yarn/ || true",
        "safe-rm-dist": "rm -rf dist/ || true",
        "safe-rm-node-modules": "rm -rf node_modules/ || true",
        "build:ts": "tsc -b --verbose",
        "build:dev": "yarn build:ts && webpack --mode=development --node-env=development",
        "build:prod": "webpack --mode=production --node-env=production",
        "build": "yarn build:prod",
        "watch": "webpack --mode=development --node-env=development --watch",
        "clean": "yarn safe-rm-ycache && yarn safe-rm-dist && yarn safe-rm-node-modules && yarn cache clean",
        "install:clean": "yarn clean && yarn install",
        "serve": "nodemon --inspect=0.0.0.0:9229 -e ts,tsx,handlebars -w src --exec \"node --experimental-specifier-resolution=node --loader ./customTSNodeLoader.js src/server/index.ts\"",
        "serve:test": "nodemon --inspect=0.0.0.0:9229 -e ts,tsx,handlebars -w src --exec \"node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"./customTSNodeLoader.js\", pathToFileURL(\"./\"));' src/server/index.ts\"",
        "start": "yarn build:dev && yarn serve",
        "restart": "yarn install:clean && yarn build:dev && yarn serve",
        "test:client": "jest --config=src/client/jest.config.js",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "debug-storybook": "yarn build-storybook --debug-webpack"
    },
    "dependencies": {
        "@chakra-ui/icons": "^2.1.1",
        "@chakra-ui/react": "^2.8.2",
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.5",
        "cors": "^2.8.5",
        "d3": "^7.9.0",
        "express": "^4.18.2",
        "express-handlebars": "^7.1.2",
        "framer-motion": "^11.0.28",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.23.1",
        "web-vitals": "^2.1.4",
        "whatwg-fetch": "^3.6.20"
    },
    "devDependencies": {
        "@babel/core": "^7.24.3",
        "@babel/node": "^7.23.9",
        "@babel/plugin-transform-runtime": "^7.24.3",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.24.3",
        "@babel/preset-react": "^7.24.1",
        "@babel/preset-typescript": "^7.24.6",
        "@chromatic-com/storybook": "^1.3.3",
        "@jest/globals": "^29.7.0",
        "@storybook/addon-essentials": "^8.0.10",
        "@storybook/addon-interactions": "^8.0.10",
        "@storybook/addon-links": "^8.0.10",
        "@storybook/addon-onboarding": "^8.0.10",
        "@storybook/addon-webpack5-compiler-swc": "^1.0.2",
        "@storybook/blocks": "^8.0.10",
        "@storybook/react": "^8.0.10",
        "@storybook/react-webpack5": "^8.0.10",
        "@storybook/test": "^8.0.10",
        "@testing-library/dom": "^10.0.0",
        "@testing-library/jest-dom": "^5.16.3",
        "@testing-library/react": "^12.1.4",
        "@testing-library/user-event": "^14",
        "@types/babel__core": "^7",
        "@types/babel__preset-env": "^7",
        "@types/cors": "^2.8.17",
        "@types/d3": "^7",
        "@types/eslint": "^8",
        "@types/express": "^4.17.21",
        "@types/jest": "^27.4.1",
        "@types/node": "^20.12.5",
        "@types/react": "^17.0.43",
        "@types/react-dom": "^17.0.14",
        "@typescript-eslint/eslint-plugin": "^7.0.1",
        "@typescript-eslint/parser": "^7.0.0",
        "@webpack-cli/generators": "^3.0.7",
        "babel-jest": "^29.7.0",
        "babel-loader": "^9.1.3",
        "concurrently": "^8.2.2",
        "copy-webpack-plugin": "^12.0.2",
        "eslint": "^8.52.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-babel": "^5.3.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^28.2.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-storybook": "^0.8.0",
        "html-webpack-plugin": "^5.6.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "nodemon": "^3.0.3",
        "prettier": "^3.1.0",
        "prettier-eslint": "^16.1.2",
        "storybook": "^8.0.10",
        "storybook-addon-remix-react-router": "^3.0.0",
        "ts-jest": "^29.1.4",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "tsconfig-paths-webpack-plugin": "^4.1.0",
        "typescript": "^5.3.3",
        "webpack": "^5.90.3",
        "webpack-assets-manifest": "^5.2.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.2",
        "webpack-node-externals": "^3.0.0",
        "workbox-webpack-plugin": "^7.0.0"
    },
    "browser": {
        "nlpssa-frontend": false
    },
    "eslintConfig": {
        "extends": [
            "plugin:storybook/recommended"
        ]
    }
}
